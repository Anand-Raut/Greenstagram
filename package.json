{
  "name": "greenstagram",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:all": "cd frontend && npm install && cd ../netlify/functions && npm install",
    "build": "cd frontend && npm run build",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:functions": "netlify functions:serve",
